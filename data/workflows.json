    "workflows": [
      {
        "id": "b8fcac08-2045-480d-954f-a8e080d988a1",
        "category": "custom",
        "owner": "me",
        "title": "Standard DFMEA Process (AIAG-VDA)",
        "summary": "A systematic approach to identifying potential design failures and implementing preventative controls.",
        "textSteps": [],
        "diagram": {
          "grid": 20,
          "zoom": 0.7578582832551992,
          "pan": {
            "x": -295.65148661334933,
            "y": -126.31066524681279
          },
          "nodes": [
            {
              "id": "start",
              "type": "start",
              "x": 540,
              "y": 90,
              "w": 260,
              "h": 80,
              "title": "1. Project Scope & Planning",
              "subtitle": "Planning Phase",
              "detail": "Define boundaries, team members, and project milestones using the 5T method."
            },
            {
              "id": "98c20cae-7a60-4bc1-b5a7-abd111d2c43e",
              "x": 540,
              "y": 180,
              "w": 260,
              "h": 80,
              "title": "2. Structure Analysis",
              "subtitle": "System Decomposition",
              "detail": "Break down the design into System, Sub-system, and Component levels using a Block Diagram."
            },
            {
              "id": "e95a5a75-068f-42b4-a159-a39e099e8741",
              "x": 720,
              "y": 270,
              "w": 260,
              "h": 80,
              "title": "3. Function Analysis",
              "subtitle": "Requirements Mapping",
              "detail": "Associate functions and requirements to each structural element identified in Step 2."
            },
            {
              "id": "76fb5df7-30b1-42c2-af66-dc0f330912fa",
              "x": 540,
              "y": 810,
              "w": 260,
              "h": 80,
              "title": "4. Failure Analysis",
              "subtitle": "Identifying Failure Modes",
              "detail": "Identify potential failure modes, effects, and causes for each design function."
            },
            {
              "id": "7b9ee914-afd2-4750-8770-12c0fb1ab514",
              "x": 720,
              "y": 1350,
              "w": 260,
              "h": 80,
              "title": "Failure Chain Review",
              "subtitle": "Cause-Effect Link",
              "detail": "Verify the logical link between Failure Cause -> Failure Mode -> Failure Effect."
            },
            {
              "id": "8428c210-4a98-4a15-8070-858f1593885f",
              "x": 720,
              "y": 1440,
              "w": 260,
              "h": 80,
              "title": "Customer Impact Study",
              "subtitle": "End User Risk",
              "detail": "Assess how the failure affects the vehicle, the user, and regulatory compliance."
            },
            {
              "id": "0479eaf3-850d-45f5-b2c9-efc180242448",
              "x": 1080,
              "y": 270,
              "w": 260,
              "h": 80,
              "title": "5. Risk Evaluation",
              "subtitle": "S / O / D Scoring",
              "detail": "Assign ratings for Severity (S), Occurrence (O), and Detection (D) based on standardized tables."
            },
            {
              "id": "b138e134-5dd3-4143-9139-1eb33241b41d",
              "x": 1440,
              "y": 360,
              "w": 260,
              "h": 80,
              "title": "High Action Priority (AP)",
              "subtitle": "Urgent Mitigation",
              "detail": "Identify items with high Severity and/or high Action Priority scores requiring immediate attention."
            },
            {
              "id": "8a9e8f8f-2925-4b8e-93a9-1f60b35fd0c7",
              "x": 1440,
              "y": 630,
              "w": 260,
              "h": 80,
              "title": "Medium Action Priority",
              "subtitle": "Evaluation Required",
              "detail": "Review design margins and current controls to see if further optimization is cost-effective."
            },
            {
              "id": "e1031095-3d1c-46dd-aa91-0252109c3108",
              "x": 1440,
              "y": 720,
              "w": 260,
              "h": 80,
              "title": "Low Action Priority",
              "subtitle": "Risk Accepted",
              "detail": "Risk is deemed acceptable; current controls are sufficient."
            },
            {
              "id": "d47c82fb-796d-4aff-880b-21bb3a66cece",
              "x": 900,
              "y": 810,
              "w": 260,
              "h": 80,
              "title": "6. Optimization",
              "subtitle": "Mitigation Strategy",
              "detail": "Define actions to reduce Severity, Occurrence, or increase Detection capability."
            },
            {
              "id": "74f0a71f-bc22-4088-bd27-0bd37db10691",
              "x": 1260,
              "y": 900,
              "w": 260,
              "h": 80,
              "title": "Physical Testing",
              "subtitle": "Detection Control",
              "detail": "Lab testing, durability cycles, or destructive testing to validate the failure point."
            },
            {
              "id": "33a0fac8-151f-442b-8a49-72285503ff56",
              "x": 1260,
              "y": 1170,
              "w": 260,
              "h": 80,
              "title": "Virtual Simulation",
              "subtitle": "Prevention Control",
              "detail": "Utilize FEA, CFD, or thermal modeling to prevent failure modes during the design phase."
            },
            {
              "id": "ad4dac9d-2406-4ac9-b174-9fc7847dd86e",
              "x": 1620,
              "y": 900,
              "w": 260,
              "h": 80,
              "title": "DVP&R Integration",
              "subtitle": "Validation Plan",
              "detail": "Incorporate detection actions into the formal Design Verification Plan and Report."
            },
            {
              "id": "253c4f26-c664-4554-971b-b5c415372d1b",
              "x": 1620,
              "y": 1170,
              "w": 260,
              "h": 80,
              "title": "Update Design Rules",
              "subtitle": "Knowledge Capture",
              "detail": "Update internal engineering standards to prevent recurrence in future programs."
            },
            {
              "id": "17ff2163-9fa1-4a60-af4b-32d07d476435",
              "x": 1620,
              "y": 1260,
              "w": 260,
              "h": 80,
              "title": "Standardization",
              "subtitle": "Final Check",
              "detail": "Apply findings to similar product families across the organization."
            },
            {
              "id": "e5d5027e-158c-4276-bb9b-7871fbec7ff1",
              "x": 1620,
              "y": 990,
              "w": 260,
              "h": 80,
              "title": "Re-evaluation of Risk",
              "subtitle": "Residual Risk Check",
              "detail": "Recalculate S/O/D after actions are implemented to ensure AP targets are met."
            },
            {
              "id": "e5ea6a26-8d60-4bde-b05f-af638eb9523d",
              "x": 1980,
              "y": 1080,
              "w": 260,
              "h": 80,
              "title": "7. Results Documentation",
              "subtitle": "Completion",
              "detail": "Finalize DFMEA report, communicate results to management, and archive the baseline."
            },
            {
              "id": "1e88f038-de13-4ebe-8f5b-1f462eeb3dd2",
              "x": 1800,
              "y": 450,
              "w": 260,
              "h": 80,
              "title": "Corrective Action Loop",
              "subtitle": "Redesign Phase",
              "detail": "Cycle back to engineering for design modifications if AP remains High."
            },
            {
              "id": "e16a094d-0431-4cd4-ab88-faa579c5aa2c",
              "x": 1980,
              "y": 540,
              "w": 260,
              "h": 80,
              "title": "Geometry Modification",
              "subtitle": "Structural Change",
              "detail": "Alter physical dimensions to reduce stress concentrations or interference."
            },
            {
              "id": "6d06d9b5-4314-4e21-8f5a-971735d19851",
              "x": 2160,
              "y": 450,
              "w": 260,
              "h": 80,
              "title": "Material Substitution",
              "subtitle": "Properties Change",
              "detail": "Select materials with higher thermal, chemical, or mechanical resistance."
            }
          ],
